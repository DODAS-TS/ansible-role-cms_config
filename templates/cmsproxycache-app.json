{
    "id": "certcache",
    "container": {
      "type": "DOCKER",
      "docker": {
        "image": "{{ cms_config_proxycache_image}}",
        "network": "BRIDGE",
        "portMappings": [{
          "containerPort": {{ cms_config_proxycache_port }},
          "servicePort": {{ cms_config_proxycache_serviceport }},
          "protocol": "tcp"
        }]
      }
    },
   "env": {
        "IAMTOKEN": "{{ cms_config_iamtoken }}"
    },
    "cmd": "/root/start_apache.sh",
    "instances": 1,
    "cpus": 1.00,
    "mem": 2048
}
